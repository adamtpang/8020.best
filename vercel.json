{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/app",
      "dest": "/"
    },
    {
      "src": "/app/(.*)",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "functions": {
    "api/*.js": {
      "maxDuration": 30,
      "memory": 1024,
      "maxPayloadSize": "50mb"
    }
  }
}